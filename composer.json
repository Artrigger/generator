{
    "name": "laravel-rocket/generator",
    "description": "Code Generator For Laravel Roclet",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Takaaki Mizuno",
            "email": "takaaki.mizuno@gmail.com"
        }
    ],
    "repositories": [
        {
          "type": "vcs",
          "url": "https://github.com/Artrigger/foundation"
        }
    ],
    "require": {
        "php": ">=7.0",
        "icanboogie/inflector": "^1.4",
        "laravel/framework": "5.5.*",
        "laravel-rocket/foundation": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "symfony/yaml": "~3.2",
        "friendsofphp/php-cs-fixer": "^2.2",
        "fzaninotto/faker": "^1.5",
        "laravel/laravel": "5.5.*",
        "doctrine/dbal": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "LaravelRocket\\Generator\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    }
}
